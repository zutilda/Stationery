<Window x:Class="Stationery.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stationery"
        mc:Ignorable="d"
        Title="Канцелярские принадлежности 'Пиши-стирай'" Height="450" Width="800" Icon="/image/icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="6*"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="0" Name="Head" NavigationUIVisibility="Hidden"/>
        <Frame Grid.Row="1" Name="Basic" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
